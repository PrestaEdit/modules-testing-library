{
  "name": "prestashop_test_module",
  "version": "0.0.3",
  "description": "retro compat resolver",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PrestaShopCorp/template-tests-modules.git"
  },
  "author": "PrestaShop",
  "license": "OSL-3.0",
  "npm-link": "https://www.npmjs.com/package/prestashop_test_module",
  "bugs": {
    "url": "https://github.com/PrestaShopCorp/template-tests-modules/issues"
  },
  "homepage": "https://github.com/PrestaShopCorp/template-tests-modules#readme",
  "scripts": {
    "start:test": "yarn start:test:lint && ./node_modules/mocha/bin/mocha --recursive --file kernel/utils/setup.js tests",
    "start:test:ui": "./node_modules/mocha/bin/mocha --recursive --file kernel/utils/setup.js tests/ui",
    "start:test:units": "./node_modules/mocha/bin/mocha --recursive tests/unit",
    "start:test:lint": "eslint  --fix --ignore-path .gitignore .",
    "lint": "eslint  --fix --ignore-path .gitignore ."
  },
  "_moduleAliases": {
    "@resolvers": "kernel/resolvers",
    "@versions": "versions",
    "@utils": "kernel/utils",
    "@kernel": "kernel",
    "@unitTests": "tests/unit",
    "@uiTests":  "tests/ui"
  },
  "dependencies": {
    "module-alias": "^2.2.2",
    "js-image-generator": "^1.0.2",
    "playwright": "^1.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "eslint-config-prestashop": "0.0.2",
    "faker": "^4.1.0",
    "mocha": "^6.1.4",
    "mochawesome": "^2.3.0",
    "pdfjs-dist": "^2.3.200",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.2"
  }
}
